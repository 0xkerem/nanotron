#SBATCH --qos=high
#SBATCH --array=0
#SBATCH -o /fsx/phuc/project_data/doremi/logs/data/doremi-%j-%a-%x.out

export XDG_CACHE_HOME=/fsx/phuc/.cache/huggingface_cache

